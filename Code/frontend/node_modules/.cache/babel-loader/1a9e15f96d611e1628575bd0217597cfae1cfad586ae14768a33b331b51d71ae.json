  cmnt: null,\n      auth_token: null,\n      error: \"\",\n    };\n  },\n  async created() {\n    this.auth_token = sessionStorage.getItem(\"auth-token\");\n    this.username = sessionStorage.getItem(\"username\");\n    this.blogid = this.$route.params.id;\n    console.log(this.blogid);\n    if (!this.auth_token) {\n      alert(\"Please Login to see your dashboard.\");\n      this.$router.push(\"/\");\n    }\n    return fetch(`http://127.0.0.1:5000/api/comment/${this.blogid}`, {\n      method: \"GET\",\n      mode: \"cors\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        \"Access-Control-Allow-Origin\": \"*\",\n        \"Authentication-Token\": `${this.auth_token}`,\n      },\n    })\n      .then((response) => response.json())\n      .then((data) => {\n        this.comments = data[\"comments\"];\n        // console.log(data[\"blogs\"]);\n        // console.log(this.comments);\n        // console.log(data);\n      })\n      .catch((error) => console.log(\"1st\", error));\n  },\n  methods: {\n    async comment() {\n      const res = await fetch(\n        `http://127.0.0.1:5000/api/comment/${this.blogid}`,\n        {\n          method: \"POST\",\n          mode: \"cors\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"Access-Control-Allow-Origin\": \"*\",\n            \"Authentication-Token\": `${this.auth_token}`,\n          },\n          body: JSON.stringify({\n            user: this.username,\n            cmnt: this.cmnt,\n            is_authenticated: true,\n          }),\n        }\n      );\n      if (res.ok) {\n        this.$router.go();\n        window.location.reload();\n        console.log(\"commentd\");\n        // this.success = \"Followed.\";\n      } else if (res.status === 400) {\n        console.log(\"error!!\");\n      }\n    },\n    async del(id) {\n      // var txt;\n      if (confirm(\"Want to delete this comment !!\")) {\n        const res = await fetch(`http://127.0.0.1:5000/api/comment/${id}`, {\n          method: \"DELETE\",\n          mode: \"cors\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"Access-Control-Allow-Origin\": \"*\",\n            \"Authentication-Token\": `${this.auth_token}`,\n          },\n          // body: JSON.stringify({\n          //   user: this.username,\n          //   cmnt: this.cmnt,\n          //   is_authenticated: true,\n          // }),\n        });\n        if (res.ok) {\n          this.$router.go();\n          window.location.reload();\n          console.log(\"commentd\");\n          // this.success = \"Followed.\";\n        } else if (res.status === 400) {\n          console.log(\"error!!\");\n        }\n        console.log(\"You pressed OK!\");\n      } else {\n        console.log(\"You pressed Cancel!\");\n        this.$router.go();\n        window.location.reload();\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.comments {\n  /* border: 1px solid red; */\n  min-height: 91vh;\n  margin: 0px;\n  position: relative;\n  background: linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4)),\n    url(\"../assets/s3.png\") no-repeat center/cover;\n}\n\n.bck {\n  text-decoration: none;\n  display: block;\n  color: azure;\n  position: absolute;\n  right: 10px;\n  top: 10px;\n  /* font-weight: 800; */\n  font-size: 1.5rem;\n  font-family: \"Rubik Spray Paint\", cursive;\n  border-radius: 20px;\n  box-shadow: 0px 2px 6px #5ce7ff;\n  padding: 8px;\n}\n.bck:hover {\n  background: #3b3a3a;\n}\n.inputarea {\n  border: 1px solid blue;\n  top: 10px;\n  position: absolute;\n  left: 420px;\n  width: 700px;\n  text-align: center;\n  font-size: 1.5rem;\n  overflow: hidden;\n}\n.inputarea textarea {\n  color: white;\n  background: transparent;\n  resize: none;\n  font-size: 1.5rem;\n  font-weight: 800;\n  width: 99%;\n  margin: 0px;\n  font-family: \"Indie Flower\", cursive;\n  box-shadow: 0px 5px 15px gainsboro;\n}\n.inputarea input {\n  font-size: 1.4rem;\n  border-radius: 20px;\n  font-family: \"Rubik Spray Paint\", cursive;\n  padding: 8px;\n  background: gainsboro;\n}\n.inputarea input:hover {\n  background: #5ce7ff;\n  color: #383737;\n}\n\n.showarea {\n  border: 1px solid red;\n  width: 700px;\n  left: 420px;\n  top: 230px;\n  position: absolute;\n  min-height: 60vh;\n  max-height: 60vh;\n  overflow: auto;\n  margin-bottom: 0px;\n  padding-bottom: 0px;\n}\n\n.coment {\n  position: relative;\n  text-align: left;\n  width: 675px;\n  /* border: 1px solid red; */\n  color: aliceblue;\n  font-size: 1.5rem;\n  font-weight: 900;\n  font-family: \"Indie Flower\", cursive;\n  margin: 20px 0px;\n  padding: 10px;\n}\n\n.fa-trash {\n  color: bisque;\n}\n.fa-trash:hover {\n  color: red;\n  cursor: pointer;\n}\n\n#inf {\n  float: left;\n  font-size: 1.2rem;\n  color: bisque;\n}\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAI;;EAERA,KAAK,EAAC;AAAU;gEACnBC,mBAAA,CAAsC;EAAnCD,KAAK,EAAC,KAAK;EAACE,IAAI,EAAC;GAAQ,QAAM;;EAE7BF,KAAK,EAAC;AAAW;;EACdG,MAAM,EAAC;AAAM;;EAMhBH,KAAK,EAAC;AAAU;;EACbA,KAAK,EAAC;AAAI;;EAINI,EAAE,EAAC;AAAK;gEACXH,mBAAA,CAA4B;EAAzBD,KAAK,EAAC;AAAa;;EAItBK,KAAwB,EAAxB;IAAA;EAAA,CAAwB;EAACH,IAAI,EAAC;;;iEAOjCD,mBAAA,CAAM;;;;uBA7BhBK,mBAAA,CAqCM,OArCNC,UAqCM,GApCJC,YAAA,CAA2CC,iBAAA;IAAlCC,QAAQ,OAAOA;yCACxBT,mBAAA,CAkCM,OAlCNU,UAkCM,GAjCJC,UAAsC,EAEtCX,mBAAA,CAMM,OANNY,UAMM,GALJZ,mBAAA,CAIO,QAJPa,UAIO,G,gBAHLb,mBAAA,CAA4D;oEAApCc,IAAI,GAAAC,MAAA;IAAEC,IAAI,EAAC,IAAI;IAACC,IAAI,EAAC;sDAArBH,IAAI,E,GAC5Bd,mBAAA,CAAkE;IAA3DkB,IAAI,EAAC,QAAQ;IAACC,KAAK,EAAC,SAAS;IAAEC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,cAAA,CAAAP,MAAA,IAAUQ,QAAA,CAAAC,OAAO;MAC5DC,mBAAA,yCAA0C,C,KAG9CzB,mBAAA,CAqBM,OArBN0B,UAqBM,GApBJ1B,mBAAA,CAmBO,QAnBP2B,UAmBO,GAlBLF,mBAAA,uDAC2B,G,kBAC3BpB,mBAAA,CAeMuB,SAAA,QAAAC,WAAA,CAfcC,KAAA,CAAAC,QAAQ,EAAhBjB,IAAI;yBAAhBT,mBAAA,CAeM;MAfyB2B,GAAG,EAAElB,IAAI,CAACX,EAAE;MAAEJ,KAAK,EAAC;QACjDC,mBAAA,CAGC,QAHDiC,UAGC,GAFEC,UAA4B,E,iBAAA,GAAC,GAAAC,gBAAA,CAAGrB,IAAI,gBAAe,GACpD,GAAAqB,gBAAA,CAAGrB,IAAI,YAAW,GAAC,GAAAqB,gBA