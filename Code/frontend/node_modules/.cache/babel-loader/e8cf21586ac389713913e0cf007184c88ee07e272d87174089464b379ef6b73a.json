{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"LoginSignup\",\n  data() {\n    return {\n      email: null,\n      username: null,\n      password: null,\n      error_1: \"\",\n      error_2: \"\"\n    };\n  },\n  async created() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  async updated() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  methods: {\n    async register() {\n      if (this.emailValidation(this.email) && this.passValidation(this.password)) {\n        try {\n          fetch(\"http://127.0.0.1:5000/api/user\", {\n            method: \"POST\",\n            headers: {\n              \"Access-Control-Allow-Origin\": \"*\",\n              \"Content-Type\": \"application/json\"\n            },\n            body: JSON.stringify({\n              email: this.email,\n              username: this.username,\n              password: this.password\n            })\n          }).then(response => response.json()).then(async data => {\n            console.log(data);\n            this.$router.go(-1);\n          }).catch(err => {\n            console.log(err);\n          });\n        } catch (error) {\n          console.log(\"Registration Failed: \", error);\n        }\n      } else if (!this.emailValidation(this.email)) {\n        this.error_1 = \"Please enter a valid email\";\n      } else if (!this.passValidation(this.password)) {\n        this.error_1 = \"Password requires atleast 8 characters.\";\n      }\n    },\n    emailValidation: function (email) {\n      var result = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,3}))$/;\n      return result.test(email);\n    },\n    passValidation: function (passs) {\n      var result = /.{8,}/;\n      return result.test(passs);\n    }\n  },\n  async login() {\n    try {\n      fetch(\"http://127.0.0.1:5000/login?include_auth_token\", {\n        method: \"POST\",\n        headers: {\n          \"Access-Control-Allow-Origin\": \"*\",\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          email: this.email,\n          password: this.password,\n          is_authenticated: true\n        })\n      }).then(resp => resp.json()).then(async data => {\n        const {\n          response\n        } = data;\n        console.log(data);\n        if (response.errors) {\n          if (response.errors[1]) {\n            this.error_1 = response.errors[1];\n          }\n          this.error_2 = response.errors[0];\n          console.log(this.error_1, this.error_2);\n        } else {\n          this.auth = response.user.authentication_token;\n          sessionStorage.setItem(\"auth-token\", this.auth);\n          sessionStorage.setItem(\"email\", this.email);\n          this.$router.push(\"home\");\n          console.log(\"its homepage\");\n        }\n      }).catch(error => {\n        console.log(\"some error first time\", error);\n      });\n    } catch (error) {\n      console.log(\"No way home: \", error);\n    }\n  }\n};","map":{"version":3,"names":["name","data","email","username","password","error_1","error_2","created","sessionStorage","clear","localStorage","updated","methods","register","emailValidation","passValidation","fetch","method","headers","body","JSON","stringify","then","response","json","console","log","$router","go","catch","err","error","result","test","passs","login","is_authenticated","resp","errors","auth","user","authentication_token","setItem","push"],"sources":["/home/agustus/mad2-project/frontend/src/components/LoginSignup.vue"],"sourcesContent":["<template>\n  <div class=\"bd\">\n    <link\n      rel=\"stylesheet\"\n      href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css\"\n    />\n    <div id=\"popup\">\n      <p class=\"alert alert-danger\" v-if=\"error_1\">{{ error_1 }}</p>\n      <p class=\"alert alert-danger\" v-if=\"error_1\">{{ error_2 }}</p>\n      <!-- {% with messages=get_flashed_messages(with_categories=true) %} {% if\n      messages %} {% for category, message in messages%}\n      <div\n        class=\"alert alert-{{ category }} alert-dismissible fade show\"\n        role=\"alert\"\n      >\n        {{ message }}\n        <button\n          type=\"button\"\n          class=\"btn-close\"\n          data-bs-dismiss=\"alert\"\n          aria-label=\"Close\"\n        ></button>\n      </div>\n      {% endfor %} {% endif %} {% endwith %} -->\n    </div>\n\n    <div id=\"main\">\n      <input class=\"out\" type=\"checkbox\" id=\"ck\" aria-hidden=\"true\" />\n\n      <!--                         SIGNUP PAGE                    -->\n\n      <div class=\"signup\">\n        <form method=\"Post\">\n          <label class=\"lb\" for=\"ck\" aria-hidden=\"true\">Sign up</label>\n          <input\n            class=\"out\"\n            type=\"text\"\n            name=\"user_nm\"\n            placeholder=\"Enter User Name*\"\n            required\n          />\n          <input\n            class=\"out\"\n            type=\"email\"\n            name=\"signup_mail\"\n            placeholder=\"Enter Email Id*\"\n            required\n          />\n          <input\n            class=\"out\"\n            type=\"password\"\n            name=\"signup_pswd1\"\n            placeholder=\"Enter Password*\"\n            required\n          />\n          <input\n            class=\"out\"\n            type=\"password\"\n            name=\"signup_pswd2\"\n            placeholder=\"Confirm Password*\"\n            required\n          />\n          <button type=\"submit\" class=\"tk\">Sign up</button>\n        </form>\n      </div>\n\n      <!--                         LOGIN PAGE                            -->\n\n      <div class=\"login\">\n        <form method=\"Post\">\n          <label class=\"lb\" for=\"ck\" aria-hidden=\"true\">Login</label>\n          <input\n            class=\"out\"\n            type=\"email\"\n            name=\"login_mail\"\n            placeholder=\"Email Id\"\n            required\n          />\n          <input\n            class=\"out\"\n            type=\"password\"\n            name=\"login_pswd\"\n            placeholder=\"Password\"\n            required\n          />\n          <button type=\"submit\" class=\"tk\">Login</button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"LoginSignup\",\n  data() {\n    return {\n      email: null,\n      username: null,\n      password: null,\n      error_1: \"\",\n      error_2: \"\",\n    };\n  },\n\n  async created() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  async updated() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  methods: {\n    async register() {\n      if (\n        this.emailValidation(this.email) &&\n        this.passValidation(this.password)\n      ) {\n        try {\n          fetch(\"http://127.0.0.1:5000/api/user\", {\n            method: \"POST\",\n            headers: {\n              \"Access-Control-Allow-Origin\": \"*\",\n              \"Content-Type\": \"application/json\",\n            },\n            body: JSON.stringify({\n              email: this.email,\n              username: this.username,\n              password: this.password,\n            }),\n          })\n            .then((response) => response.json())\n            .then(async (data) => {\n              console.log(data);\n              this.$router.go(-1);\n            })\n            .catch((err) => {\n              console.log(err);\n            });\n        } catch (error) {\n          console.log(\"Registration Failed: \", error);\n        }\n      } else if (!this.emailValidation(this.email)) {\n        this.error_1 = \"Please enter a valid email\";\n      } else if (!this.passValidation(this.password)) {\n        this.error_1 = \"Password requires atleast 8 characters.\";\n      }\n    },\n    emailValidation: function (email) {\n      var result =\n        /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,3}))$/;\n      return result.test(email);\n    },\n    passValidation: function (passs) {\n      var result = /.{8,}/;\n      return result.test(passs);\n    },\n  },\n\n  async login() {\n    try {\n      fetch(\"http://127.0.0.1:5000/login?include_auth_token\", {\n        method: \"POST\",\n        headers: {\n          \"Access-Control-Allow-Origin\": \"*\",\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify({\n          email: this.email,\n          password: this.password,\n          is_authenticated: true,\n        }),\n      })\n        .then((resp) => resp.json())\n        .then(async (data) => {\n          const { response } = data;\n          console.log(data);\n          if (response.errors) {\n            if (response.errors[1]) {\n              this.error_1 = response.errors[1];\n            }\n            this.error_2 = response.errors[0];\n            console.log(this.error_1, this.error_2);\n          } else {\n            this.auth = response.user.authentication_token;\n            sessionStorage.setItem(\"auth-token\", this.auth);\n            sessionStorage.setItem(\"email\", this.email);\n            this.$router.push(\"home\");\n            console.log(\"its homepage\");\n          }\n        })\n        .catch((error) => {\n          console.log(\"some error first time\", error);\n        });\n    } catch (error) {\n      console.log(\"No way home: \", error);\n    }\n  },\n};\n</script>\n<!-- <script\n  src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js\"\n  integrity=\"sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8\"\n  crossorigin=\"anonymous\"\n></script> -->\n\n<style scoped>\n.bd {\n  mar