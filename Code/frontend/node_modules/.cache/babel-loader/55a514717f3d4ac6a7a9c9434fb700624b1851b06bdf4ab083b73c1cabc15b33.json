outer.push(\"home\");\n              console.log(\"its homepage\");\n            }\n          })\n          .catch((error) => {\n            console.log(\"some error first time\", error);\n          });\n      } catch (error) {\n        console.log(\"No way home: \", error);\n      }\n    },\n  },\n};\n</script>\n"],"mappings":";AAkBA;AACA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AACtD;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH,SAAS;IACTC;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,CAAC,EAAE,IAAI;MACPC,UAAU,EAAE;QACVC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,IAAI;QACdC,SAAS,EAAE,IAAI;QACfC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX,CAAC;MACDC,SAAS,EAAE;QACTN,KAAK,EAAE,IAAI;QACXO,QAAQ,EAAE,IAAI;QACdH,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXG,IAAI,EAAE,IAAI;QACVC,gBAAgB,EAAE;MACpB;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACdC,UAAU,CAAC,MAAO,IAAI,CAACb,CAAA,GAAI,KAAM,EAAE,IAAI,CAAC;IAExCc,cAAc,CAACC,KAAK,EAAE;IACtBC,YAAY,CAACD,KAAK,EAAE;EACtB,CAAC;EACD;;EAEA;EACA,MAAME,OAAOA,CAAA,EAAG;IACdH,cAAc,CAACC,KAAK,EAAE;IACtBC,YAAY,CAACD,KAAK,EAAE;EACtB,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACfC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACpB,UAAU,CAAC;MAC5B,IACE,IAAI,CAACqB,eAAe,CAAC,IAAI,CAACrB,UAAU,CAACC,KAAK,KAC1C,IAAI,CAACqB,cAAc,CAAC,IAAI,CAACtB,UAAU,CAACG,SAAS,KAC7C,IAAI,CAACoB,gBAAgB,CACnB,IAAI,CAACvB,UAAU,CAACG,SAAS,EACzB,IAAI,CAACH,UAAU,CAACI,SAAQ,CAC1B,EACA;QACA,IAAI;UACFoB,KAAK,CAAC,gCAAgC,EAAE;YACtCC,IAAI,EAAE,MAAM;YACZC,MAAM,EAAE,MAAM;YACdC,OAAO,EAAE;cACP,6BAA6B,EAAE,GAAG;cAClC,cAAc,EAAE;YAClB,CAAC;YACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;cACnB7B,KAAK,EAAE,IAAI,CAACD,UAAU,CAACC,KAAK;cAC5BC,QAAQ,EAAE,IAAI,CAACF,UAAU,CAACE,QAAQ;cAClCC,SAAS,EAAE,IAAI,CAACH,UAAU,CAACG;YAC7B,CAAC;UACH,CAAC,EACE4B,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAC,MAAOjC,IAAI,IAAK;YACpBqB,OAAO,CAACC,GAAG,CAACtB,IAAI,CAAC;YACjB,IAAI,CAACoC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC;YACnBC,KAAK,CAAC,yCAAyC,CAAC;UAClD,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;YA