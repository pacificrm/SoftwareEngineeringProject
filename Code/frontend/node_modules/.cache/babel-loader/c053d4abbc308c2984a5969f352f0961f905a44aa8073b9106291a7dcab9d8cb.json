{"ast":null,"code":"// @ is an alias to /src\nimport StartPage from \"@/components/StartPage.vue\";\nimport LoginSignup from \"@/components/LoginSignup.vue\";\n// import { clearInterval } from \"timers\";\n\nexport default {\n  name: \"StartView\",\n  components: {\n    StartPage,\n    LoginSignup\n  },\n  data() {\n    return {\n      t: true,\n      signupData: {\n        email: null,\n        username: null,\n        password1: null,\n        password2: null,\n        error_1: \"\",\n        error_2: \"\"\n      }\n    };\n  },\n  async created() {\n    setTimeout(() => this.t = false, 7000);\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  // async created() {\n\n  // },\n  async updated() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  methods: {\n    async register() {\n      console.log(this.signupData);\n      if (this.emailValidation(this.signupData.email) && this.passValidation(this.signupData.password1) && this.passConfirmation(this.signupData.password1, this.signupData.password2)) {\n        try {\n          fetch(\"http://127.0.0.1:5000/api/user\", {\n            mode: \"cors\",\n            method: \"POST\",\n            headers: {\n              \"Access-Control-Allow-Origin\": \"*\",\n              \"Content-Type\": \"application/json\"\n            },\n            body: JSON.stringify({\n              email: this.signupData.email,\n              username: this.signupData.username,\n              password1: this.signupData.password1\n            })\n          }).then(response => response.json()).then(async data => {\n            console.log(data);\n            this.$router.go(-1);\n          }).catch(err => {\n            console.log(err);\n          });\n        } catch (error) {\n          console.log(\"Registration Failed: \", error);\n        }\n      } else if (!this.emailValidation(this.signupData.email)) {\n        this.error_1 = \"Please enter a valid email\";\n      } else if (!this.passValidation(this.signupData.password1)) {\n        this.error_1 = \"Password requires atleast 8 characters.\";\n      } else if (this.signupData.password1, this.signupData.password2) {\n        this.error_1 = \"Password didn't matched!!\";\n      }\n    },\n    emailValidation: function (email) {\n      var result = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,3}))$/;\n      return result.test(email);\n    },\n    passValidation: function (passs) {\n      var result = /.{8,}/;\n      return result.test(passs);\n    },\n    passConfirmation: function (passs1, passs2) {\n      if (passs1 == passs2) {\n        return true;\n      }\n      return false;\n    }\n  }\n};","map":{"version":3,"names":["StartPage","LoginSignup","name","components","data","t","signupData","email","username","password1","password2","error_1","error_2","created","setTimeout","sessionStorage","clear","localStorage","updated","methods","register","console","log","emailValidation","passValidation","passConfirmation","fetch","mode","method","headers","body","JSON","stringify","then","response","json","$router","go","catch","err","error","result","test","passs","passs1","passs2"],"sources":["/home/agustus/mad2-project/frontend/src/views/StartView.vue"],"sourcesContent":["<template>\n  <div class=\"start\">\n    <!-- <h1>This is an about page</h1> -->\n    <!-- <StartPage /> -->\n    <StartPage v-show=\"t\" />\n    <LoginSignup v-show=\"!t\" @signup=\"(signupData = $event), register()\" />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport StartPage from \"@/components/StartPage.vue\";\nimport LoginSignup from \"@/components/LoginSignup.vue\";\n// import { clearInterval } from \"timers\";\n\nexport default {\n  name: \"StartView\",\n  components: {\n    StartPage,\n    LoginSignup,\n  },\n\n  data() {\n    return {\n      t: true,\n      signupData: {\n        email: null,\n        username: null,\n        password1: null,\n        password2: null,\n        error_1: \"\",\n        error_2: \"\",\n      },\n    };\n  },\n  async created() {\n    setTimeout(() => (this.t = false), 7000);\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  // async created() {\n\n  // },\n  async updated() {\n    sessionStorage.clear();\n    localStorage.clear();\n  },\n  methods: {\n    async register() {\n      console.log(this.signupData);\n      if (\n        this.emailValidation(this.signupData.email) &&\n        this.passValidation(this.signupData.password1) &&\n        this.passConfirmation(\n          this.signupData.password1,\n          this.signupData.password2\n        )\n      ) {\n        try {\n          fetch(\"http://127.0.0.1:5000/api/user\", {\n            mode: \"cors\",\n            method: \"POST\",\n            headers: {\n              \"Access-Control-Allow-Origin\": \"*\",\n              \"Content-Type\": \"application/json\",\n            },\n            body: JSON.stringify({\n              email: this.signupData.email,\n              username: this.signupData.username,\n              password1: this.signupData.password1,\n            }),\n          })\n            .then((response) => response.json())\n            .then(async (data) => {\n              console.log(data);\n              this.$router.go(-1);\n            })\n            .catch((err) => {\n              console.log(err);\n            });\n        } catch (error) {\n          console.log(\"Registration Failed: \", error);\n        }\n      } else if (!this.emailValidation(this.signupData.email)) {\n        this.error_1 = \"Please enter a valid email\";\n      } else if (!this.passValidation(this.signupData.password1)) {\n        this.error_1 = \"Password requires atleast 8 characters.\";\n      } else if ((this.signupData.password1, this.signupData.password2)) {\n        this.error_1 = \"Password didn't matched!!\";\n      }\n    },\n    emailValidation: function (email) {\n      var result =\n        /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,3}))$/;\n      return result.test(email);\n    },\n    passValidation: function (passs) {\n      var result = /.{8,}/;\n      return result.test(passs);\n    },\n    passConfirmation: function (passs1, passs2) {\n      if (passs1 == passs2) {\n        return true;\n      }\n      return false;\n    },\n  },\n};\n</script>\n"],"mappings":"AAUA;AACA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AACtD;;AAEA,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH,SAAS;IACTC;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,CAAC,EAAE,IAAI;MACPC,UAAU,EAAE;QACVC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,IAAI;QACdC,SAAS,EAAE,IAAI;QACfC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACdC,UAAU,CAAC,MAAO,IAAI,CAACT,CAAA,GAAI,KAAM,EAAE,IAAI,CAAC;IACxCU,cAAc,CAACC,KAAK,EAAE;IACtBC,YAAY,CAACD,KAAK,EAAE;EACtB,CAAC;EACD;;EAEA;EACA,MAAME,OAAOA,CAAA,EAAG;IACdH,cAAc,CAACC,KAAK,EAAE;IACtBC,YAAY,CAACD,KAAK,EAAE;EACtB,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACfC,OAAO,CAACC,GAAG,CAAC,IAAI,CAAChB,UAAU,CAAC;MAC5B,IACE,IAAI,CAACiB,eAAe,CAAC,IAAI,CAACjB,UAAU,CAACC,KAAK,KAC1C,IAAI,CAACiB,cAAc,CAAC,IAAI,CAAClB,UAAU,CAACG,SAAS,KAC7C,IAAI,CAACgB,gBAAgB,CACnB,IAAI,CAACnB,UAAU,CAACG,SAAS,EACzB,IAAI,CAACH,UAAU,CAACI,SAAQ,CAC1B,EACA;QACA,IAAI;UACFgB,KAAK,CAAC,gCAAgC,EAAE;YACtCC,IAAI,EAAE,MAAM;YACZC,MAAM,EAAE,MAAM;YACdC,OAAO,EAAE;cACP,6BAA6B,EAAE,GAAG;cAClC,cAAc,EAAE;YAClB,CAAC;YACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;cACnBzB,KAAK,EAAE,IAAI,CAACD,UAAU,CAACC,KAAK;cAC5BC,QAAQ,EAAE,IAAI,CAACF,UAAU,CAACE,QAAQ;cAClCC,SAAS,EAAE,IAAI,CAACH,UAAU,CAACG;YAC7B,CAAC;UACH,CAAC,EACEwB,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAC,MAAO7B,IAAI,IAAK;YACpBiB,OAAO,CAACC,GAAG,CAAClB,IAAI,CAAC;YACjB,IAAI,CAACgC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC;UACrB,CAA