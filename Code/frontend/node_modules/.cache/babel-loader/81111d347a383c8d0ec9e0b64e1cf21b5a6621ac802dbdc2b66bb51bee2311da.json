{"ast":null,"code":"// @ is an alias to /src\n// import HelloWorld from \"@/components/HelloWorld.vue\";\nimport NavBar from \"@/components/NavBar.vue\";\nexport default {\n  name: \"PostBlog\",\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      Blogdata: {\n        blogTitle: null,\n        blogPrev: null,\n        bolgContent: null,\n        blogImage: null\n      }\n    };\n  },\n  methods: {\n    async uploadImage(event) {\n      this.Blogdata.blogImage = event.target.files[0];\n    },\n    async postBlog() {\n      let formData = new FormData();\n      formData.append(\"file\", this.Blogdata.blogImage);\n      formData.append(\"file\", this.Blogdata.blogTitle);\n      formData.append(\"file\", this.Blogdata.blogPrev);\n      formData.append(\"file\", this.Blogdata.blogContent);\n      console.log(formData);\n      try {\n        const res = await fetch(\"http://127.0.0.1:5000/api/postblog\", {\n          mode: \"cors\",\n          method: \"POST\",\n          headers: {\n            \"Access-Control-Allow-Origin\": \"*\",\n            \"Content-Type\": \"multipart/form-data\",\n            Accept: \"application/jason\"\n          },\n          data: formData\n        });\n        if (res.ok) {\n          console.log(\"Blog-Posted\");\n        }\n      } catch (error) {\n        console.log(\"Cannot post \", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","name","components","data","Blogdata","blogTitle","blogPrev","bolgContent","blogImage","methods","uploadImage","event","target","files","postBlog","formData","FormData","append","blogContent","console","log","res","fetch","mode","method","headers","Accept","ok","error"],"sources":["/home/agustus/mad2-project/frontend/src/components/PostBlog.vue"],"sourcesContent":["<template>\n  <NavBar></NavBar>\n  <div class=\"postBlog\">\n    <form\n      action=\"http://127.0.0.1:8080/postblog\"\n      enctype=\"multipart/form-data\"\n      method=\"POST\"\n    >\n      <div class=\"pp\">\n        <label for=\"blogTitle\" maxlength=\"80\"> Blog Title</label>\n        <!-- <input id=\"pt\" type=\"textarea\" name=\"postTitle\" required /> -->\n        <textarea\n          v-model=\"blogTitle\"\n          id=\"pt\"\n          cols=\"500\"\n          rows=\"1\"\n          required\n        ></textarea>\n      </div>\n      <div class=\"pp\">\n        <label for=\"blogPreview\"> Blog preview </label>\n        <!-- <input id=\"ppp\" type=\"textarea\" name=\"postPreview\" required /> -->\n        <textarea\n          v-model=\"blogPrev\"\n          id=\"ppp\"\n          cols=\"500\"\n          rows=\"4\"\n          required\n        ></textarea>\n      </div>\n      <div class=\"pp\">\n        <label for=\"blogContent\"> Write Your Blog </label>\n        <!-- <input id=\"pc\" type=\"textarea\" name=\"postContent\" required /> -->\n        <textarea\n          v-model=\"blogContent\"\n          cols=\"56\"\n          id=\"pc\"\n          rows=\"10\"\n          required\n        ></textarea>\n      </div>\n      <div class=\"pp\">\n        <input id=\"pi\" type=\"file\" @change=\"uploadImage()\" />\n      </div>\n      <div class=\"pp\">\n        <input\n          id=\"pb\"\n          type=\"submit\"\n          value=\"Post-Blog\"\n          @click.prevent=\"postBlog()\"\n        />\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n// import HelloWorld from \"@/components/HelloWorld.vue\";\nimport NavBar from \"@/components/NavBar.vue\";\n\nexport default {\n  name: \"PostBlog\",\n  components: {\n    NavBar,\n  },\n  data() {\n    return {\n      Blogdata: {\n        blogTitle: null,\n        blogPrev: null,\n        bolgContent: null,\n        blogImage: null,\n      },\n    };\n  },\n  methods: {\n    async uploadImage(event) {\n      this.Blogdata.blogImage = event.target.files[0];\n    },\n\n    async postBlog() {\n      let formData = new FormData();\n      formData.append(\"file\", this.Blogdata.blogImage);\n      formData.append(\"file\", this.Blogdata.blogTitle);\n      formData.append(\"file\", this.Blogdata.blogPrev);\n      formData.append(\"file\", this.Blogdata.blogContent);\n      console.log(formData);\n      try {\n        const res = await fetch(\"http://127.0.0.1:5000/api/postblog\", {\n          mode: \"cors\",\n          method: \"POST\",\n          headers: {\n            \"Access-Control-Allow-Origin\": \"*\",\n            \"Content-Type\": \"multipart/form-data\",\n            Accept: \"application/jason\",\n          },\n          data: formData,\n        });\n        if (res.ok) {\n          console.log(\"Blog-Posted\");\n        }\n      } catch (error) {\n        console.log(\"Cannot post \", error);\n      }\n    },\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Rubik+Bubbles&display=swap\");\n.postBlog {\n  top: 56px;\n  /* left: 20px; */\n  font-family: \"Rubik Bubbles\", \"cursive\";\n  color: rgb(221, 245, 9);\n  position: absolute;\n  width: 96vw;\n  height: 621px;\n  /* left: 10%; */\n  margin-top: 10px;\n  background: linear-gradient(rgba(45, 3, 80, 0.6), rgba(4, 12, 56, 0.6)),\n    url(\"../assets/pb3.jpg\") no-repeat center/cover;\n  border: 1px solid red;\n  padding: 30px;\n  font-size: 1.7rem;\n}\n.postBlog label {\n  -webkit-text-stroke-width: 1px;\n  -webkit-text-stroke-color: #9c077c;\n  /* font-weight: bold; */\n}\n\n/* .postBlog input {\n  height: 30px;\n  margin: 10px;\n  width: 200px;\n} */\nform {\n  margin: 0px;\n  padding: 0px;\n}\n.pp {\n  /* border: 1px solid green; */\n  display: flex;\n  flex-direction: row;\n  margin: 10px 20px 10px;\n  position: relative;\n  min-height: 35px;\n}\n/* .pp label {\n  font-size: 2rem;\n} */\n.pp input {\n  margin-left: 10px;\n  font-size: 1.8rem;\n  font-family: \"Rubik Bubbles\", \"cursive\";\n}\n.pp textarea {\n  font-size: 1.5rem;\n  margin-left: 10px;\n  resize: none;\n  color: rgb(223, 236, 240);\n  background: transparent;\n  box-shadow: 2px 2px 2px 4px rgb(39, 3, 59);\n}\n#pt {\n  font-size: 2rem;\n  width: 1200px;\n  margin-left: 93px;\n  margin-bottom: 20px;\n  font-weight: bold;\n}\n\n#ppp {\n  font-size: 1.5rem;\n  width: 1201px;\n  margin-left: 43px;\n  margin-bottom: 20px;\n  font-weight: bold;\n}\n\n#pc {\n  width: 1199px;\n  font-size: 1.5rem;\n  margin-bottom: 20px;\n  font-weight: bold;\n}\n\n#pi {\n  position: absolute;\n  left: 620px;\n  -webkit-text-stroke-width: 1px;\n  -webkit-text-stroke-color: #9c077c;\n}\n\n#pb {\n  margin-top: 8px;\n  position: absolute;\n  left: 680px;\n  cursor: pointer;\n  font-size: 2rem;\n  -webkit-text-stroke-width: 1px;\n  -webkit-text-stroke-color: #9c077c;\n}\n\n#pb:hover {\n  background: rgb(53, 52, 52);\n  color: white;\n  box-shadow: 4px 4px 8px rgb(228, 153, 5);\n}\n</style>\n"],"mappings":"AAyDA;AACA;AACA,OAAOA,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,SAAS,EAAE,IAAI;QACfC,QAAQ,EAAE,IAAI;QACdC,WAAW,EAAE,IAAI;QACjBC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAACC,KAAK,EAAE;MACvB,IAAI,CAACP,QAAQ,CAACI,SAAQ,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACjD,CAAC;IAED,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAIC,QAAO,GAAI,IAAIC,QAAQ,EAAE;MAC7BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACb,QAAQ,CAACI,SAAS,CAAC;MAChDO,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACb,QAAQ,CAACC,SAAS,CAAC;MAChDU,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACb,QAAQ,CAACE,QAAQ,CAAC;MAC/CS,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACb,QAAQ,CAACc,WAAW,CAAC;MAClDC,OAAO,CAACC,GAAG,CAACL,QAAQ,CAAC;MACrB,IAAI;QACF,MAAMM,GAAE,GAAI,MAAMC,KAAK,CAAC,oCAAoC,EAAE;UAC5DC,IAAI,EAAE,MAAM;UACZC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,6BAA6B,EAAE,GAAG;YAClC,cAAc,EAAE,qBAAqB;YACrCC,MAAM,EAAE;UACV,CAAC;UACDvB,IAAI,EAAEY;QACR,CAAC,CAAC;QACF,IAAIM,GAAG,CAACM,EAAE,EAAE;UACVR,OAAO,CAACC,GAAG,CAAC,aAAa,CAAC;QAC5B;MACF,EAAE,OAAOQ,KAAK,EAAE;QACdT,OAAO,CAACC,GAAG,CAAC,cAAc,EAAEQ,KAAK,CAAC;MACpC;IACF;EACF;AACF,CAAC"},"metadat